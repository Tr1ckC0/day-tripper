<%= form_with model: @user do |f| %>
    <%= f.label :first_name %><br />
    <%= f.text_field :first_name %><br /><br />

    <%= f.label :last_name %><br />
    <%= f.text_field :last_name %><br /><br />

    <%= f.label :email %><br />
    <%= f.text_field :email %><br /><br />

    <%= f.label :password %><br />
    <%= f.password_field :password_digest %><br /><br />

    <%= f.submit "Create Account"%> <%# Needs to be changed to support both edit and new%>
<% end %>