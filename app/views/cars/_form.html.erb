<%= form_with model: @car do |f| %>
    <%= f.label :make %><br />
    <%= f.text_field :make %><br /><br />

    <%= f.label :model %><br />
    <%= f.text_field :model %><br /><br />

    <%= f.label :year %><br />
    <%= f.text_field :year %><br /><br />

    <%= f.label "Number of Seats" %><br />
    <%= f.number_field :num_of_seats, max: 8 %><br /><br />

    <%= f.label :color %><br />
    <%= f.text_field :color %><br /><br />

    <%= f.label :license_plate %><br />
    <%= f.text_field :license %><br /><br />

    <%= f.submit "Add Car"%>
<% end %>